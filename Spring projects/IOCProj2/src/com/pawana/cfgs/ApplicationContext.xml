<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
       
   <bean id="st1" class="com.pawana.model.Student">
   		<property name="id" value="10"/>
   		<property name="name" value="Anvesh"/>
   		<property name="address" value="Ameerpet"/>
   </bean>    
   <bean id="st" class="com.pawana.model.Student">
   		<constructor-arg type="java.lang.String" value="Pavani"/>
   		<constructor-arg type="java.lang.String" value="Hyd"/>
   		<constructor-arg type="int" value="12"/>
   </bean>
</beans>